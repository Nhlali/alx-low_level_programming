#include "main.h"

/**
* jack_bauer - function that prints every minute of the day of Jack Bauer,
* starting from 00:00 to 23:59.
*
*/
void jack_bauer(void)
{

	int h1;
	int h2;
	int m1;
	int m2;
	int i;
	
	for (h1 = 48; h1 <= 50; h1++)
	{

		if (h1 == 2)
		{
			i = 51;
		}
		else 
		{
			i = 57;
		}
		
		for (h2 = 48; h2 <= i; h2++)
		{
			for (m1 = 48; m1 <= 54; m1++)
			{
				for (m2 = 48; m2 <= 57; m2++)
				{	
					_putchar(h1);
					_putchar(h2);
					_putchar(':');
					_putchar(m1);
					_putchar(m2);
					_putchar('\n');
				}
			}
		}
	}
}
